    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_search">

    <fragment
        android:id="@+id/navigation_learn"
        android:name="org.semmelweis.university.anatomyapp.ui.learn.LearnFragment"
        tools:layout="@layout/fragment_learn" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="org.semmelweis.university.anatomyapp.ui.search.SearchFragment"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_puzzle"
        android:name="org.semmelweis.university.anatomyapp.ui.puzzle.PuzzleFragment"
        tools:layout="@layout/fragment_puzzle" />

    <fragment
        android:id="@+id/navigation_findparts"
        android:name="org.semmelweis.university.anatomyapp.ui.findparts.FindpartsFragment"
